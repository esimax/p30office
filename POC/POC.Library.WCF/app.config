            <endpoint address="net.tcp://localhost:19001/VoiceRecord" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IProtocolVoiceRecord"
                contract="ProxyVoiceRecord.IProtocolVoiceRecord" name="NetTcpBinding_IProtocolVoiceRecord" />
            <endpoint address="net.tcp://localhost:19001/Messaging" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IProtocolMessagingServer"
                contract="ProxyMessaging.IProtocolMessagingServer" name="NetTcpBinding_IProtocolMessagingServer" />
            <endpoint address="net.tcp://localhost:19001/PhoneMonitoring"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IProtocolPhoneMonitoring"
                contract="ProxyPhoneMonitoring.IProtocolPhoneMonitoring" name="NetTcpBinding_IProtocolPhoneMonitoring" />
        </client>
    </system.serviceModel>
